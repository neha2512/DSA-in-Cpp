#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
int i,n,sum1=0,sum2=0;
    cin>>n;
    for(i=1;i<=n;i++)
    {
        if(i%2!=0)
        {
            sum1=sum1+i;
    }
    
    else{
        sum2=sum2+i;
    }
    }
    cout<<sum1<<" "<<sum2;
    return 0;
}
