#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {   
    string s;
int c=0,c1=0,c2=0;
    getline(cin,s);
      int  n=s.length();
    for(int i=0;i<n;i++){
        
        if(s[i]=='a'||s[i]=='e'||s[i]=='i'||s[i]=='o'||s[i]=='u'||s[i]=='A'||s[i]=='E'||s[i]=='I'||s[i]=='O'||s[i]=='U')
        {
            c++;
    }
else if(s[i]>='0' && s[i]<='9'){
    c2++;
    }
else{
    c1++;
}
    }
cout<<c<<" "<<c1<<" "<<c2;
    return 0;
}
